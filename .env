# Ejemplo de archivo .env para KrakenBot en NAS
# Copia este archivo a .env y configura los valores

# Credenciales de base de datos
DB_PASSWORD=KrakenBot2024Seguro
DATABASE_URL=postgres://krakenbot:${DB_PASSWORD}@localhost:5432/krakenbot

# API de Kraken (se configuran desde la app, pero puedes ponerlos aquí también)
KRAKEN_API_KEY=
KRAKEN_API_SECRET=

# Telegram (se configuran desde la app, pero puedes ponerlos aquí también)
TELEGRAM_BOT_TOKEN=
TELEGRAM_CHAT_ID=

# Tokens de WebSocket (OBLIGATORIOS para Monitor y Terminal)
WS_ADMIN_TOKEN=tu-token-admin-seguro
TERMINAL_TOKEN=tu-token-terminal-seguro
